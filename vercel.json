{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "dist/index.js"
    },
    {
      "src": "/api-docs(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/dist/index.js" }
  ]
}

